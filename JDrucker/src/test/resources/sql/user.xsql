# users table sql scripts
dialect: mysql

init {
    CREATE TABLE `users` (
        `id` INT NOT NULL,
        `name` VARCHAR(50) NOT NULL,
        `gender` VARCHAR(50) NOT NULL,
        `created_at` DATETIME NOT NULL,
        `updated_at` DATETIME NOT NULL
    ) ENGINE = InnoDB;
}

