# users table definition

dialect: mysql;

initialize users {
}

migrations {
}

# Find users by created_at range.
mysql sql query_users {
    SELECT * FROM users
        WHERE `created_at` BETWEEN '2019-05-09 00:00:00' AND '2019-10-09 23:59:59';
}

# Find users by dynamic date range.
sql query_users2 {
    SELECT * FROM users
        WHERE `created_at` BETWEEN '${begin_at}' AND '${end_at}';
}